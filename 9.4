name = input("Enter file:")
if len(name) < 1 : name = "mbox-short.txt"
handle = open(name)
counts = dict()
for line in handle:
    if line.startswith("From "):
        words = line.split()
        pers = words[1]
        counts[pers] = counts.get(pers,0) + 1


bigw = None
bigc = None

for key in counts:
    if bigc is None or counts[key] > bigc:
        bigw = key
        bigc = counts[key]


print(bigw, bigc)
