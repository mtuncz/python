def is_even(p):
    n = len(p)
    ct = 0 
    i = 0
    while(i < n):
        j = i + 1
        while(j<n):
            if p[i] > p[j]:
                ct += 1
            j = j + 1
        i = i + 1

    if ct % 2 == 0:
        return True
    else:
        return False
        

print(is_even([4, 9, 3, 1, 5, 2, 7, 8, 0, 6]))
print(is_even([0,3,2,4,5,6,7,1,9,8] ))
